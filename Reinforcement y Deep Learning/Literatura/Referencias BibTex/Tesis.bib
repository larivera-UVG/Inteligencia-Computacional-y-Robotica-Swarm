journal = {Journal of Intelligent and Robotic Systems: Theory and Applications},
keywords = {Group behavior,Potential functions,Swarm systems},
number = {4},
pages = {369--394},
title = {{Decentralized control of autonomous swarm systems using artificial potential functions: Analytical design guidelines}},
volume = {45},
year = {2006}
}
