journal = {Ingenier{\'{i}}a e Investigaci{\'{o}}n},
keywords = {Algorithme {\'{e}}volutionniste,Artificial intelligence,Automatic control,Base de donn{\'{e}}es,Bibliographic search,Commande automatique,Database,Evolutionary algorithm,Image processing,Intelligence artificielle,Intelligence en essaim,Recherche bibliographique,Robotics,Robotique,Signal processing,Syst{\'{e}}matique,Taxonomy,Traitement image,Traitement signal,computational intelligence,evolutionary computing,optimisation algorithm,swarm intelligence},
