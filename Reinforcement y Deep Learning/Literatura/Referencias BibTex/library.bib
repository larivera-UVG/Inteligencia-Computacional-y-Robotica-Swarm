doi = {10.1007/978-3-319-53480-0_104},
file = {:C$\backslash$:/Users/eddys/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Le{\'{o}}n et al. - 2017 - Robot swarms theory applicable to seek and rescue operation.pdf:pdf},
isbn = {9783319534794},
